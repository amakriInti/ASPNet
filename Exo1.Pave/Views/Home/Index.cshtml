
@{
    ViewBag.Title = "Identification";
}

<h2>Identification</h2>
@if (ViewBag.Erreur == null)
{
    <table style="width:300px">
        <tr>
            <td><button class="b1" onclick="concat('1');">1</button></td>
            <td><button class="b1" onclick="concat('2');">2</button></td>
            <td><button class="b1" onclick="concat('3');">3</button></td>
        </tr>
        <tr>
            <td><button class="b1" onclick="concat('4');">4</button></td>
            <td><button class="b1" onclick="concat('5');">5</button></td>
            <td><button class="b1" onclick="concat('6');">6</button></td>
        </tr>
        <tr>
            <td><button class="b1" onclick="concat('7');">7</button></td>
            <td><button class="b1" onclick="concat('8');">8</button></td>
            <td><button class="b1" onclick="concat('9');">9</button></td>
        </tr>
        <tr>
            <td><button class="b1" onclick="concat('#');">#</button></td>
            <td><button class="b1" onclick="concat('0');">0</button></td>
            <td><button class="b1" onclick="concat('!');">!</button></td>
        </tr>
    </table>
}
<h5 id="erreur" style="color:red">@ViewBag.Erreur</h5>
